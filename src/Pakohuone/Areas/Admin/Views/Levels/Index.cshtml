@model List<Level>

@{
    ViewData["Title"] = "Levels";
}

<partial name="_UploadModalPartial" />

<div class="row">
    <div class="col">
        <table class="table ph-table">
            <thead>
                <tr>
                    <th scope="col">@Html.DisplayNameFor(model => model[0].Name)</th>
                    <th data-orderable="false"></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var level in Model)
                {
                    <tr>
                        <td>@level.Name</td>
                        <td>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

@section ToolbarActions {
    <partial name="_UploadButtonPartial" />
}

@section Scripts {
    <script src="~/js/admin/table.js"></script>
    <script src="~/js/admin/upload.js"></script>
}
